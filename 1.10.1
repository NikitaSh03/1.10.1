class Rectangle:

    def __init__(self, x, y, width, height):
        self.x = x
        self.y = y
        self.width = width
        self.height = height

    def __str__(self):
        return (
            f"x = {self.x}; "
            f"y = {self.y}; "
            f"width = {self.width}; "
            f"height = {self.height} ")

Some_str = Rectangle(834,2342,123,2341)
print(Some_str.__str__())
